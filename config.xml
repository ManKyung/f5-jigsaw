<?xml version='1.0' encoding='utf-8'?>
<widget id="com.f5game.jigsaw" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="orientation" value="portrait"/>
  <name>F5 Jigsaw</name>
  <description>
    F5-Jigsaw Puzzle Game(Free)
  </description>
  <author email="realmojo88@gmail.com" href="https://realmojo880601.web.app/">
    realmojo88
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
  </platform>
</widget>
